---
# Database Name & Credentials to write metrics
influxdb_database: 'database'
influxdb_username: 'username'
influxdb_password: 'password'

# Docker Variables for containers
docker:
  influxdb:
    username: 'administrator' # Influxdb Admin Credentials
    password: 'password'
  grafana:
    admin_user: 'administrator' # Grafana Admin Credentials
    admin_pass: 'password'